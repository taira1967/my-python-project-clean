{
  "buildCommand": "cd myproject2-react && npm install && npm run build",
  "outputDirectory": "myproject2-react/dist",
  "devCommand": "cd myproject2-react && npm run dev",
  "installCommand": "cd myproject2-react && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
